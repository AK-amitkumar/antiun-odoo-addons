<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>

<record id="view_product_supplierinfo_print_wizard" model="ir.ui.view">
    <field name="name">Print Pricelists</field>
    <field name="model">product.supplierinfo.print.wizard</field>
    <field name="priority">15</field>
    <field name="arch" type="xml">
        <form string="Print Pricelists">
            <group string="Optional Fields">
                <field name="product_code" />
                <field name="default_code" />
                <field name="min_qty" />
                <field name="delay" />
                <field name="discount" />
            </group>
            <footer>
                <button string="Print" name="print_supplierinfo" type="object" class="oe_highlight"/>
                or
                <button string="Cancel" class="oe_link" special="cancel" />
            </footer>
        </form>
    </field>
</record>

</data>
</openerp>
