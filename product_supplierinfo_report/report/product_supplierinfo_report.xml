<?xml version="1.0" encoding="UTF-8"?>
<openerp>
<data>

<template id="product_supplierinfo_report">
    <t t-call="report.external_layout">
        <div class="page">
            <h1>Print Pricelist</h1>
            <p><t t-esc="type"/> HOLA</p>
            <div class="row">
                <div class="col-xs-6">
                </div>
                <div class="col-xs-6">
                    <div>
                        <div t-if="type=='customer'">
                            <strong>CUSTOMER:</strong>
                        </div>
                        <div t-if="type=='supplier'">
                            <strong>SUPPLIER:</strong>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </t>
</template>

</data>
</openerp>


