<?xml version="1.0" encoding="UTF-8"?>
<openerp>
<data>


<template id="report_label_product_product">
    <t t-call="report.html_container">
        <div class="page">
        <t t-foreach="docs" t-as="o">
            <div style="float:left;width:396px;height:140px;margin:5px 5px 30px 5px;padding:3px;border:solid #eee 1px;">
                <div style="float:left;width:40%;height:140px;overflow:hidden;padding-top:20px;text-align:center">
                    <img style="width:100%;height:50%;" t-att-src="'/report/barcode/Code128/%s' % o.ean13"/>
                    <div style="font-size:15px;width:100%;"><span t-field="o.ean13"/></div>
                </div>
                <div style="float:left;width:60%;height:140px;overflow:hidden;padding-top:20px;">

                    <div style="margin-left:10px"><span t-field="o.default_code"/>
                                <span t-field="o.name"/><br/><br/><br/>
                    <span t-field="o.total_price"/> EUR</div>
                </div>
            </div>
        </t>
        </div>
    </t>
</template>


</data>
</openerp>


