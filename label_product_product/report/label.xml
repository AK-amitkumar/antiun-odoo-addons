<?xml version="1.0" encoding="UTF-8"?>
<openerp>
<data>


<template id="report_label_product_product">
    <t t-call="report.html_container">
        <div class="page">
        <t t-foreach="docs" t-as="o">
            <div style="float:left;width:396px;height:140px;margin:5px 5px 30px 5px;padding:3px;border:solid #eee 1px;">
                <div style="float:left;font-size:20px;text-align:left;width:100%;padding-left:5px;">
                    <div style="float:left;width:40%;"><span t-field="o.default_code"/></div>
                    <div style="float:right;width:60%;"><span t-field="o.lst_price"/> EUR</div>
                </div>
                <div>
                    <div style="float:left;width:40%;height:50px;overflow:hidden;">
                        <img style="width:100%;height:100%;" t-att-src="'/report/barcode/Code128/%s' % o.ean13"/>
                    </div>
                    <div style="float:right;font-size:15px;text-align:left;width:60%;padding-left:5px;"><span t-field="o.name"/></div>
                    <div style="float:left;font-size:15px;margin-left:5%;width:100%;"><span t-field="o.ean13"/></div>
                </div>
            </div>
        </t>
        </div>
    </t>
</template>


</data>
</openerp>


