<?xml version="1.0" encoding="UTF-8"?>
<openerp>
<data>


<template id="report_label_product_product">
    <t t-call="report.html_container">
        <div class="page">
        <t t-foreach="docs" t-as="o">
            <div style="float:left;width:260px;height:150px;margin:5px 5px 20px 5px;padding:3px;border:solid #eee 1px;">
                    <div style="font-size:15px;text-align:center;width:100%;"><span t-field="o.name"/></div>
                    <div style="width:250px;height:100px;overflow:hidden;">
                    <img style="width:400px; height:100px;margin-left:-80px" t-att-src="'/report/barcode/Code128/%s' % o.ean13"/>
                	</div>
                    <span style="font-size:12px;margin-left:30%;" t-field="o.ean13"/>
            </div>
        </t>
        </div>
    </t>
</template>


</data>
</openerp>


